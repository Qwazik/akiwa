extends _template

block main
	// ========== main-slider =============
	.page__main-slider
		.container.main-slider
			.main-slider__slider
				#mainSlider
					.main-slider__item
						.main-slider__text.fadeIn
							strong Продвижение, поддержка и 
							u разработка сайтов
						.main-slider__image
							.person.line
								.name Дмитрий Горский
								.spec Топ менеджер «Акива»
							img(src="img/main-slider__man.png", alt="")
					.main-slider__item
						.main-slider__text
							strong Продвижение, поддержка и 
							u разработка сайтов
						.main-slider__image
							.person 
								.name Дмитрий Горский
								.spec Топ менеджер «Акива»
							img(src="img/main-slider__man.png", alt="")
					.main-slider__item
						.main-slider__text
							strong Продвижение, поддержка и 
							u разработка сайтов
						.main-slider__image
							.person 
								.name Дмитрий Горский
								.spec Топ менеджер «Акива»
							img(src="img/main-slider__man.png", alt="")
			.main-slider__callback.callback-form
				i.icon.icon-freeanaliz
				.callback-form__header
					strong Бесплатный анализ сайта
					span Мы перезвоним в течении 30 минут
				form(action="#")
					input.input(type="text" placeholder="Ваше имя")
					input.input(type="text" placeholder="Адрес вашего сайта")
					input.input(type="text" placeholder="Телефон")
					button.submit(type="submit") Отправить
	// ========== main-slider end =========
	// ========== service =========
	.page__service
		.container
			.service.clearfix
				.col-md-12
					h2.service__header.big Услуги
				.row.service__list
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-grow
						.service__name Продвижение сайтов
						.service__body
							ul
								li: a(href="#") По позициям
								li: a(href="#") По трафику
					.col-md-4.col-sm-6.service__item-wrap: .service__item.mark
						.service__icon
							i.icon.icon-lidgen
						.service__name Лидогенерация
						.service__body
							p СРА-маркетинг. Реклама, которой нет равных.
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-window
						.service__name Интернет реклама
						.service__body
							ul
								li: a(href="#") Контекстная
								li: a(href="#") Медийная
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-glob
						.service__name Веб-аналитики
						.service__body
							ul
								li: a(href="#") Call Tracking
								li: a(href="#") веб-аналитика
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-audits
						.service__name Аудиты
						.service__body
							ul
								li: a(href="#") А/Б тестирование
								li: a(href="#") юзабилити аудит
								li: a(href="#") seo-аудит
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-copywrighting
						.service__name Копирайтинг
						.service__body
							ul
								li: a(href="#") нейминг
								li: a(href="#") креатив
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-sites
						.service__name Создание сайтов
						.service__body
							ul
								li.w100: a(href="#") корпоративные сайты
								li: a(href="#") интернет-магазины          
								li: a(href="#") landing page
								li: a(href="#") мобильные версии         
								li: a(href="#") адаптивный дизайн
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-corpport
						.service__name Корпоративные порталы
						.service__body
							p Любой сложности и их дальнейшее сопровождение
					.col-md-4.col-sm-6.service__item-wrap: .service__item
						.service__icon
							i.icon.icon-design
						.service__name Дизайн
						.service__body
							ul
								li: a(href="#") логотипы         
								li: a(href="#") полиграфия        
								li: a(href="#") брендбук
								li: a(href="#") презентации
			.service-buttons
				.col-md-4.col-sm-6.bdr
					a(href="").btn.btn--thin 
						i.icon.icon-adobe
						span Скачать прайс-лист
				.col-md-8.col-sm-6
					a(href="").btn.btn--thin.presentation
						i.icon.icon-akiwa
						span Скачать презентацию
						p.desc Узнайте о нас и нашей работе!
	// ========== service end =========
	// ========== team  =========
	.page__team
		.container
			.team.clearfix
				.big-header.clearfix
					h2.big Команда
					a(href="#").vacancy.right
						i.icon.icon-vacancy
						span Есть вакансии!
				.row
					.col-md-6.relative
						.team__item.big.clearfix
							.inner
								.image
									img(src="img/team1.png", alt="")
								.text
									.name Александр Балаганов
									.spec Руководитель Akiwa
									.desc.line.line--left Основная миссия нашего<br>коллектива — организация сервиса одного окна. 
									a(href="").btn.btn--red Узнать больше о нас
					.col-md-6
						.team__item.clearfix
							.text
								.name Дмитрий Горский
								.spec Топ менеджер
								p.desc
									span.num + 12
									span.red Вежливых и компетентных менеджеров 
									| в 7 городах России
							.image
								img(src="img/team2.png", alt="")
						.team__item.clearfix
							.image
								img(src="img/team3.png", alt="")
							.text
								.name Ситанислав Борисенко
								.spec Глава техподдержки
								p.desc.line.line--left
									span.red Я знаю все о техническом сопровождении сайтов
							.support-callback
								form(action="")
									p Попросить техподдержку связаться с Вами
									input.input(type="text" placeholder="Ваш телефон")
									input(type="submit" value="Отправить заявку").btn.btn--brg
					
			.team-footer.clearfix
				.team-footer__item
					.image
						img(src="img/team4.png", alt="")
					.text.line.line--left
						.name Сергей Лобановский
						.spec Гуру продвижения сайтов
				.team-footer__item
					.image
						img(src="img/team5.png", alt="")
					.text.line.line--left
						.name Виктор Ковалев
						.spec Ведущий программист
				.team-footer__item
					.image
						img(src="img/team6.png", alt="")
					.text.line.line--left
						.name Алиса Крамер
						.spec Арт директор
				.team-footer__more.team-footer__item
					img(src="img/team-footer__mans.png", alt="")
					.num <span>+</span>86
					p специалистов в области продвижения, поддержки и разработки сайтов в России
			.col-md-12.ta-center
				a(href="#").btn.btn--default Больше о нас							
	// ========== team end =========
	// ========== achivements =========
	.page__achivements
		.container
			h2.big Достижения
			.achivements
				include includes/achivements-item
				include includes/achivements-item
				include includes/achivements-item
			.achivements-controls#achivementsControls
				a(href="#").prev: img(src="data:image/png;base64,", alt="")
				.achivements-controls__info
					span Достижение
					span.position-num 3
				a(href="#").next: img(src="data:image/png;base64,", alt="")
	// ========== achivements end =========
	// ========== shema =========
	.page__shema
		.container
			h2.big Схема работы

			.shema
				.shema__item.shema__item--1 
					img(src="img/shema-item1.png", alt="")
					span 1
					.tooltip Разработка уникольной стратегии продвижения
				.shema__item.shema__item--2 
					img(src="img/shema-item2.png", alt="")
					span 2
					.tooltip.tooltip--bottom Подбор релевантного списка запросов
				.shema__item.shema__item--3 
					img(src="img/shema-item3.png", alt="")
					span 3
					.tooltip Оптимизация дизайна, структуры и контента сайта
				.shema__item.shema__item--4 
					img(src="img/shema-item4.png", alt="")
					span 4
					.tooltip.tooltip--bottom Работа по наращиванию эффективной ссылочной массы
				.shema__item.shema__item--5 
					img(src="img/shema-item5.png", alt="")
					span 5
					.tooltip Анализ алгоритмов ПС, мониторинг позиций
				.shema__item.shema__item--6 
					img(src="img/shema-item6.png", alt="")
					span 6
					.tooltip.tooltip--bottom Корректировка стратегии продвижения
				.shema__item.shema__item--7 
					img(src="img/shema-item7.png", alt="")
					span 7
					.tooltip Успех гарантирован. Ваш сайт в ТОПе!
			.shema-bottom
				.manager-callback
					span Есть вопросы ?
					img(src="img/manager-callback.png", alt="")
					a(href="#") Я отвечу на <br> любой!
				a(href="#").btn.btn--red.fl-right Хочу заказать продвижение
